<div class="container-fluid container-fluid-custom pb-5 mb-5">
  <div class="row">
    <% if(searchResults !='' ) { %>
      <% searchResults.forEach(function (bookmark, index) { %>
        <a href="/bookmark/item/<%= bookmark._id %>">
          <h4>
            <%= bookmark.title %>
          </h4>
        </a>
        <% }) %>
          <% } else { %>
            <p>No items found.</p>
            <% } %>
  </div>
</div>